%h4 Overall stats
%ul
  %li
    Total:
    %strong= pluralize @counts[:total], 'pipeline'
  %li
    Successful:
    %strong= pluralize @counts[:success], 'pipeline'
  %li
    Failed:
    %strong= pluralize @counts[:failed], 'pipeline'
  %li
    Success ratio:
    %strong
      #{success_ratio(@counts)}%
